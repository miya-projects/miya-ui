{
  "name": "miya-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng s -o",
    "build": "npm run ng-high-memory build -- --configuration production",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "hmr": "ng s -o --hmr",
    "analyze": "npm run ng-high-memory build -- --configuration production --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^14.2.8",
    "@angular/animations": "^14.2.9",
    "@angular/common": "^14.2.9",
    "@angular/compiler": "^14.2.9",
    "@angular/core": "^14.2.9",
    "@angular/forms": "^14.2.9",
    "@angular/platform-browser": "^14.2.9",
    "@angular/platform-browser-dynamic": "^14.2.9",
    "@angular/router": "^14.2.9",
    "@ant-design/icons-angular": "^13.0.1",
    "@delon/abc": "^14.1.1",
    "@delon/acl": "^14.1.1",
    "@delon/auth": "^14.1.1",
    "@delon/cache": "^14.1.1",
    "@delon/chart": "^14.1.1",
    "@delon/form": "^14.1.1",
    "@delon/mock": "^14.1.1",
    "@delon/theme": "^14.1.1",
    "@delon/util": "^14.1.1",
    "@types/qs": "^6.9.5",
    "ajv": "^8.8.2",
    "ajv-formats": "^2.1.1",
    "core-js": "^3.20.2",
    "date-fns": "^2.28.0",
    "ng-alain": "^14.2.0",
    "ng-zorro-antd": "^14.0.0",
    "ngx-tinymce": "^13.0.0",
    "ngx-ueditor": "^13.0.0",
    "qs": "^6.9.6",
    "rxjs": "~7.5.0",
    "screenfull": "^5.1.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-eslint/builder": "~14.0.3",
    "@angular-eslint/eslint-plugin": "~14.0.3",
    "@angular-eslint/eslint-plugin-template": "~14.0.3",
    "@angular-eslint/schematics": "~14.0.3",
    "@angular-eslint/template-parser": "~14.0.3",
    "@angular/cli": "^14.2.8",
    "@angular/compiler-cli": "^14.2.9",
    "@angular/language-service": "^14.2.0",
    "@delon/testing": "^14.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "~5.35.1",
    "@typescript-eslint/parser": "~5.35.1",
    "codelyzer": "^6.0.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-deprecation": "~1.3.2",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jsdoc": "~39.3.6",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-prettier": "~4.2.1",
    "hash.js": "^1.1.7",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ng-alain-plugin-theme": "^13.0.3",
    "ng-alain-sts": "^0.0.1",
    "prettier": "^2.7.1",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.8.4",
    "webpack": "^5.44.0"
  },
  "lint-staged": {
    "(src)/**/*.{html,ts}": [
      "eslint --fix"
    ],
    "(src)/**/*.less": [
      "stylelint --syntax less --fix"
    ]
  }
}
