{
  "name": "miya-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng s -o",
    "build": "npm run ng-high-memory build -- --configuration production",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "hmr": "ng s -o --hmr",
    "analyze": "npm run ng-high-memory build -- --configuration production --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^16.1.5",
    "@angular/animations": "^16.1.6",
    "@angular/common": "^16.1.6",
    "@angular/compiler": "^16.1.6",
    "@angular/core": "^16.1.6",
    "@angular/forms": "^16.1.6",
    "@angular/platform-browser": "^16.1.6",
    "@angular/platform-browser-dynamic": "^16.1.6",
    "@angular/router": "^16.1.6",
    "@ant-design/icons-angular": "^16.0.0",
    "@delon/abc": "^16.2.0",
    "@delon/acl": "^16.2.0",
    "@delon/auth": "^16.2.0",
    "@delon/cache": "^16.2.0",
    "@delon/chart": "^16.2.0",
    "@delon/form": "^16.2.0",
    "@delon/mock": "^16.2.0",
    "@delon/theme": "^16.2.0",
    "@delon/util": "^16.2.0",
    "@types/qs": "^6.9.5",
    "ajv": "^8.8.2",
    "ajv-formats": "^2.1.1",
    "core-js": "^3.20.2",
    "date-fns": "^2.28.0",
    "ng-alain": "^16.2.0",
    "ng-zorro-antd": "^16.1.0",
    "ngx-tinymce": "^13.0.0",
    "ngx-ueditor": "^13.0.0",
    "qs": "^6.9.6",
    "rxjs": "~7.8.0",
    "screenfull": "^5.1.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "~16.1.0",
    "@angular-eslint/eslint-plugin": "~16.1.0",
    "@angular-eslint/eslint-plugin-template": "~16.1.0",
    "@angular-eslint/schematics": "~16.1.0",
    "@angular-eslint/template-parser": "~16.1.0",
    "@angular/cli": "^16.1.5",
    "@angular/compiler-cli": "^16.1.6",
    "@angular/language-service": "^16.1.0",
    "@delon/testing": "^16.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "~6.1.0",
    "@typescript-eslint/parser": "~6.1.0",
    "codelyzer": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-jsdoc": "~46.4.4",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-prettier": "~5.0.0",
    "hash.js": "^1.1.7",
    "husky": "^7.0.4",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "prettier": "^3.0.0",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^13.8.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~5.1.6",
    "webpack": "^5.44.0",
    "eslint-plugin-deprecation": "~1.4.1",
    "ng-alain": "^16.2.0",
    "ng-alain-plugin-theme": "^15.0.1"
  },
  "lint-staged": {
    "(src)/**/*.{html,ts}": [
      "eslint --fix"
    ],
    "(src)/**/*.less": [
      "stylelint --syntax less --fix"
    ]
  }
}
